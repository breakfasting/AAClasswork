<h1>Log in</h1>

<form action="<%= sessions_url %>" method="POST">
  <%= render 'layouts/auth' %>
  <label>
    Email:
    <input type="text" name="user[email]">
  </label>
  <label>
    Password:
    <input type="password" name="user[password]">
  </label>
  <input type="submit" value="Login!">
</form>